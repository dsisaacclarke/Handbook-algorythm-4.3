#include <iostream>
#include <climits>
#include <vector>
#include <algorithm>

int main() {
    
    int n;
    std::cin >> n;

    std::vector<int> a(n);
    for (int i = 0; i < n; ++i) {
        std::cin >> a[i];
    }

    std::sort(a.begin(), a.end());
    long long max_prod = LLONG_MIN;

    if(n == 4){
     max_prod = (long long)a[0] * a[1] * a[2] * a[3];
    }
     else {
         long long p1 = (long long)a[n-1]*a[n-2]*a[n-3]*a[n-4];
         long long p2 = (long long)a[0]*a[1]*a[n-1]*a[n-2];
         long long p3 = (long long)a[0]*a[1]*a[2]*a[3];
         
         max_prod = std::max({p1,p2,p3});
        }

    std::cout << max_prod << "\n";
}
