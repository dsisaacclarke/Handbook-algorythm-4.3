#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    int n;
    std::cin >> n;

    std::vector<int> numbers(n);
    for (int i = 0; i < n; ++i) {
        std::cin >> numbers[i];
    }

    int max1 = 0;
    int max2 = 0;

    for(int num : numbers){
        if(num > max1){
            max2 = max1;
            max1 = num;
        }
        else if (num > max2){
            max2 = num;
        }
    }

    long long max_product = static_cast<long long>(max1) * max2;

    std::cout << max_product << "\n";
}
